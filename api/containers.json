{
    "name":  "icevision",
    "image":  "467433242231.dkr.ecr.us-west-2.amazonaws.com/icevision-api:latest",
    "cpu":  0,
    "portMappings":  [
                         {
                             "containerPort":  8000,
                             "hostPort":  8000,
                             "protocol":  "tcp"
                         }
                     ],
    "essential":  true,
    "environment":  [
                        {
                            "name":  "MODEL_PATH",
