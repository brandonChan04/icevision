                        },
                        {
                            "name":  "IMG_SIZE",
                            "value":  "864"
                        }
                    ],
    "mountPoints":  [

                    ],
    "volumesFrom":  [

                    ],
    "logConfiguration":  {
                             "logDriver":  "awslogs",
                             "options":  {
                                             "awslogs-group":  "/ecs/icevision-api",
                                             "awslogs-region":  "us-west-2",
                                             "awslogs-stream-prefix":  "ecs"
                                         }
                         },
    "systemControls":  [

                       ]
}
